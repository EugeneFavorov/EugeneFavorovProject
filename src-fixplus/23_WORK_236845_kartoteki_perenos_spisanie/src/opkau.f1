ELSE IF LASTKEY EQ KEYCODE ("F1")
THEN DO ON ENDKEY UNDO, NEXT ACTION:
   IF AVAIL op THEN 
   DO:
      RUN op#vs.p (op.op-date,op.user-id,op.op,level + 1).
   END.
END.
