{qrdef.i
   &buff-list        = "pl_indocsreg"
   &need-buff-list   = "pl_indocsreg"
   &Join-list        = "each"
   &fixed-where           = "'where pl_indocsreg.doc_type = ""ПТС"" 
                         AND pl_indocsreg.file_name eq ""term-obl""
                         AND pl_indocsreg.surrogate eq ContCode'"
   &var-where        = "ContCode"
   &SortBy          = "'BY RECID(pl_indocsreg)'"
   &condition        = "true"
}
