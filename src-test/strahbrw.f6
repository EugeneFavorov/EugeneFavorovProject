ELSE IF LASTKEY EQ KEYCODE("F6") THEN
DO ON ERROR UNDO, LEAVE
   ON ENDKEY UNDO,LEAVE : 
   PAUSE 0.
   UPDATE
      mFltCustCat
      mFltCustID
      mFltInn
      mFltShortName
      mFltName
      mFltRaschAcct
      mFltCorrAcct 
      mFltBankName
      mFltTelefax
      mFltBankBIC
   WITH FRAME flt-fr.
   HIDE FRAME flt-fr NO-PAUSE.
   {relq {&files}}
   NEXT REFRESH.
END.
ELSE IF LASTKEY EQ KEYCODE("Ctrl-F6") THEN
DO:
   ASSIGN
      mFltCustCat  = ""
      mFltCustID   = ?
      mFltShortName = ""
      mFltInn      = ""
      mFltName     = ""
      mFltRaschAcct  = ?
      mFltCorrAcct = ""
      mFltBankName = ""
      mFltTelefax = ""
      mFltBankBIC = ""
   .
   {relq {&files}}
   NEXT REFRESH.
END.

