DEF BUFFER bpl_indocsreg FOR pl_indocsreg.
FIND FIRST bpl_indocsreg WHERE
    bpl_indocsreg.file_name = "term-obl"
    AND bpl_indocsreg.surrogate = ContCode 
    AND RECID(bpl_indocsreg) > RECID(pl_indocsreg) NO-LOCK NO-ERROR.
IF AVAIL bpl_indocsreg THEN DO:
    MESSAGE "Сначала удалите все последующие движения документов" VIEW-AS ALERT-BOX TITLE "Ошибка".
    NEXT action.
END.


