IF FRAME-INDEX = 1 OR FRAME-INDEX = 2 THEN DO:

 mi = FRAME-INDEX - 1.
    RUN calend.p.
         if (lastkey eq 13 or lastkey eq 10) and pick-value ne ? then 
        DISPLAY date(pick-value) @ code.misc[FRAME-INDEX] WITH FRAME edit.
        
        
