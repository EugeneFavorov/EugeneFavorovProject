DEF TEMP-TABLE ttReport NO-UNDO
    FIELD tf_id                 AS INT64 INIT 0 /* Порядковый номер строки                     */
    FIELD tf_payment-date       AS DATE         /*  1. Дата операции                           */
    FIELD tf_basic-sum-loan     AS DEC   INIT 0 /*  2. Погашение основного долга (idnt=3)      */
    FIELD tf_prosr-osn          AS DEC   INIT 0 /*  3. Погашение просроченного основного долга */
    FIELD tf_sum-percent        AS DEC   INIT 0 /*  4. Погашение процентов       (idnt=1)      */
    FIELD tf_prosr-proc         AS DEC   INIT 0 /*  5. Погашение просроченных процентов        */
    FIELD tf_proc-pr-osn        AS DEC   INIT 0 /*  6. Погашение процентов на просроченный основной долг */
    FIELD tf_peni               AS DEC   INIT 0 /*  7. Пеня                                    */
    FIELD tf_peni-pts           AS DEC   INIT 0 /*  8. Пеня за непредоставление ПТС            */
    FIELD tf_comm               AS DEC   INIT 0 /*  9. Комиссии за перевод денежных средств    */
    FIELD tf_sum-payment        AS DEC   INIT 0 /* 10. Общая сумма платежа)                    */
    FIELD tf_rest-debts         AS DEC   INIT 0 /* 11. Суммы остатков            (idnt=2)      */
    .
