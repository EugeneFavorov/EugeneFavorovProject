else if frame-field = "cont-code" and
	(lookup(key-label(lastkey),'-,/,\,_,|, ,BACKSPACE') <> 0 or
	lastkey ge 48  and lastkey le 57  or
	lastkey ge 65  and lastkey le 90  or 
	lastkey ge 97  and lastkey le 122 or 
	lastkey ge 128 and lastkey le 175 or
	lastkey ge 224 and lastkey le 239)
then do:

  	wh = h-frm[n-frm]:first-child.
	wh = wh:first-child.
  	do while valid-handle(wh):
    	if wh:name = "cont-code" 
			then leave.
    		else wh = wh:next-sibling.
  	end.                  

  	if not valid-handle(wh) then next ACTION.

  	loan_srch = if key-label(lastkey) = "BACKSPACE" 
		then substr(loan_srch, 1, length(loan_srch) - 1) 
		else (loan_srch + key-label(lastkey)).

	if key-label(lastkey) = "BACKSPACE" then do:
		wh:label = if length(loan_srch) ne 0 then loan_srch 
											 else "çéåÖê ÑéÉéÇéêÄ".
		old_loan_srch = loan_srch.
		next action.
	end.

	{getq first {&qry}}
	SRCH:
    repeat:
    	if not avail b-loan then do:
			loan_srch = old_loan_srch.
			{getq first {&qry}}
			next SRCH.		
		end.
      	if b-loan.cont-code begins loan_srch {&and} 
		then do:
			wh:label = loan_srch.
			old_loan_srch = loan_srch.
		    leave.
		end.
        else release b-loan.
	  	{getq next {&qry}}
  	end.
	next REFRESH.
end.