else if lastkey = 13 and AVAILABLE schedule then do:
  pick-value = "yes".
  {message &text="|Запустить задачу прямо сейчас?" &alert-box=question &buttons=yes-no}
  if pick-value = "yes" then do:
    run run-once.p (RECID(schedule)).
  end.
  next ACTION.
end.
else if lastkey = 10 and AVAILABLE schedule then do:
     pick-value = string(schedule.{&rfld}).
     leave REFRESH.
end.
