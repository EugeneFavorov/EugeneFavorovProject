ELSE IF LASTKEY EQ 10 AND
   TRANSACTION        AND
   favail()
   THEN
DO:
   pick-value = {&edtfile}.code.
        message "22" view-as alert-box. 
   LEAVE REFRESH.
END.


message "dddd" view-as alert-box. 