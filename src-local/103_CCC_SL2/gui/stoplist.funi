/*ccc*/
DEFINE VAR mCloseDate AS CHARACTER NO-UNDO.
mCloseDate = ?.

IF NUM-ENTRIES(DataLine.Sym1,CHR(1)) GT 3 THEN
	mCloseDate = ENTRY(4,DataLine.Sym1,CHR(1)).

/*
FOR EACH code WHERE
	code.code EQ DataLine.Sym4
	NO-LOCK,
	EACH signs WHERE TRUE
	AND signs.file-name EQ "code"
	AND signs.code      EQ "CloseDate"
	AND signs.surrogate EQ code.class + "," + code.code
	NO-LOCK:
	mCloseDate = signs.code-value.
END.
*/

IF  DATE(mCloseDate) NE ? 
AND DATE(mCloseDate) LT TODAY THEN NEXT.
/*ccc*/
