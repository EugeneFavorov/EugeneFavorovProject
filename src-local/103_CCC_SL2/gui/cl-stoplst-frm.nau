END.
ELSE IF FRAME-FIELD =  "mDataCls" THEN
DO:
   RUN calend.p.
   IF KEY-FUNCTION(LAST-KEY) <> "END-ERROR" THEN
      DISPLAY pick-value @ mDataCls WITH FRAME edit.
