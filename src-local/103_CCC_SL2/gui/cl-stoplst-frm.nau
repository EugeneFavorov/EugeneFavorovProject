ELSE IF FRAME-FIELD =  "mBirthDay" THEN
DO:
   RUN calend.p.
   IF KEY-FUNCTION(LAST-KEY) <> "END-ERROR" THEN
      mBirthDay:SCREEN-VALUE = pick-value.
END.
